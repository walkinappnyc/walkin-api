    },
    "attractions": {
      "type": "hasAndBelongsToMany",
      "model": "Attraction",
      "foreignKey": ""
    },
    "attraction": {
      "type": "belongsTo",
      "model": "Attraction",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
