{
  "name": "Neighborhood",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "string",
      "dataType": "text"
    },
    "weight": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "created_at": {
      "type": "date"
    },
    "updated_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "pointsofInterests": {
      "type": "hasMany",
      "model": "PointsofInterest",
      "foreignKey": "",
      "through": "NeighborhoodPointOfInterest"
    },
    "region": {
      "type": "belongsTo",
      "model": "Region",
      "foreignkey": ""
    },
    "buildings": {
      "type": "hasMany",
      "model": "Building",
      "foreignKey": ""
    },
    "pointofInterests": {
      "type": "hasMany",
      "model": "PointofInterest",
      "foreignKey": "",
      "through": "NeighborhoodPointOfInterest"
    },
    "metadatas": {
      "type": "hasOne",
      "model": "MetaData",
      "polymorphic": "metadatable"
    },
    "addressables": {
      "type": "hasOne",
      "model": "Address",
      "polymorphic": "addressable"
    }
  },
  "acls": [],
  "methods": {}
}
