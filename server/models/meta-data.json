{
  "name": "MetaData",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "keywords": {
      "type": "string"
    },
    "robots": {
      "type": "string"
    },
    "created_at": {
      "type": "date"
    },
    "updated_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "metadatable": {
      "type": "belongsTo",
      "polymorphic": true
    }
  },
  "acls": [{
